{
  "eslint.autoFixOnSave":  true,
  "eslint.validate":  [
    "javascript",
    "javascriptreact",
    {"language": "vue", "autoFix": true },
    {"language": "typescript", "autoFix": true },
    {"language": "typescriptreact", "autoFix": true }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "i18n-ally.localesPaths": [
    "src/languages"
  ],
}
